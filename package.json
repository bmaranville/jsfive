{
  "name": "jsfive",
  "version": "0.3.8",
  "description": "A pure javascript HDF5 file reader, based on pyfive",
  "main": "./dist/index.js",
  "type": "module",
  "exports": "./dist/index.js",
  "browser": "./dist/hdf5.js",
  "scripts": {
    "build": "npm run build_browser && npm run build_esm",
    "build_browser": "esbuild index.js --bundle --sourcemap --target=es2020 --outfile=dist/hdf5.js --format=iife --global-name=hdf5",
    "build_esm": "esbuild index.js --bundle --format=esm --outfile=dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usnistgov/jsfive"
  },
  "keywords": [
    "hdf5",
    "javascript",
    "es6",
    "browser"
  ],
  "author": "Brian B. Maranville",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/usnistgov/jsfive/issues"
  },
  "homepage": "https://github.com/usnistgov/jsfive#readme",
  "dependencies": {
    "pako": "^2.0.4"
  },
  "devDependencies": {
    "esbuild": "^0.14.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
